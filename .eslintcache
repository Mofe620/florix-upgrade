                     <div className=\"d-grid gap-2\">\r\n                                <Button\r\n                                    type='button'\r\n                                    size=\"lg\"\r\n                                    disabled={cartItems.length === 0}\r\n                                    onClick={checkoutHandler}\r\n                                >\r\n                                    Checkout\r\n                                </Button>\r\n                            </div>\r\n                        </ListGroup.Item>\r\n                    <ListGroup.Item>\r\n                        <div className=\"d-grid gap-2\">\r\n                            <Button\r\n                                type='button'\r\n                                size=\"lg\"\r\n                                variant=\"warning\"\r\n                                onClick={keepShopping}\r\n                            >\r\n                                Add More Products?\r\n                            </Button>\r\n                        </div>\r\n                    </ListGroup.Item>\r\n                    </ListGroup>\r\n                </Card>\r\n            </Col>\r\n        </Row>\r\n        </Container>\r\n             <Footer />\r\n        </>\r\n    )\r\n}\r\n\r\nconst mapStateToProps = state => ({\r\n    isAuthenticated: state.auth.isAuthenticated\r\n  });\r\n  \r\n  export default connect(mapStateToProps, { addToCart, removeFromCart}) (CartScreen)","C:\\Users\\chukwuyem\\projects\\florix\\florix-frontend\\src\\screens\\orderScreens\\PlaceOrderScreen.js",["550","551","552"],"import React, { useEffect } from 'react'\r\nimport Header from '../../components/global/Header';\r\nimport Footer from '../../components/Footer';\r\nimport { Button, Row, Col, ListGroup, Image, Card, Container } from 'react-bootstrap'\r\nimport { Link, useLocation } from 'react-router-dom'\r\nimport { useDispatch, useSelector, connect } from 'react-redux'\r\nimport Message from '../../